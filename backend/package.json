    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "nodemailer": "^6.9.7",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
